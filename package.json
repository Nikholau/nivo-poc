{
  "name": "mundoinvest-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:test": "next dev -p 3001",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "prepare": "husky install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests",
    "test:ci": "npm test -- --ci --maxWorkers=2",
    "test:watch": "npm test -- --watchAll",
    "test:staged": "npm test -- --findRelatedTests",
    "test:cypress": "cypress open",
    "test:cypress:run": "cypress run",
    "test:cypress:ci": "cypress run --quiet --config baseUrl=http://localhost:3000",
    "test:e2e:ci": "start-server-and-test dev 3000 test:cypress:ci",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages -- --out=storybook-static --ci --existing-output-dir=storybook-static"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
